// Gmsh project created on Fri Jan 30 02:46:24 2015
height = DefineNumber[ 10, Name "Parameters/height" ];
width = DefineNumber[ 100, Name "Parameters/width" ];

Point(1) = {-0.5*width, 0.5*height, 0, 2.5};
Point(2) = {-0.5*width, -0.5*height, 0, 2.5};

Line(1) = {1, 2};
width = DefineNumber[ 100, Name "Parameters/width" ];
Extrude {width, 0, 0} {
  Line{1}; Layers{10};
}

depth = DefineNumber[ 40, Name "Parameters/depth" ];

Extrude {0, 0, -depth} {
  Surface{5}; Layers{10};
}


